	<parameter name="BANQUE" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     SUM(paie.NET) AS salaire,
     paramgen.nomEntreprise AS paramgen_nomEntreprise,
     paie.noCompteBanque AS paie_noCompteBanque,
     employe.nni AS employe_nni,
     employe.nom AS employe_nom,
     employe.id AS employe_id,
     paramgen.logoSociete AS paramgen_logoSociete,
     paramgen.bankvirement AS paramgen_bankvirement,
     paramgen.comptevirement AS paramgen_comptevirement,
     employe.prenom AS employe_prenom
FROM
     paie paie INNER JOIN paramgen paramgen ON paie.paramgen = paramgen.id
     INNER JOIN employe employe ON paie.employe = employe.id
WHERE
     paie.periode = $P{PERIODE}
 AND paie.banque = $P{BANQUE}
GROUP BY
     paie.employe,
     paie.noCompteBanque
ORDER BY
     paie.employe ASC]]>
	</queryString>
